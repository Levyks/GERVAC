{
  "ts-node": {
      "files": true
  },
"compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "typeRoots": ["./typings"],
    "paths": {
        "*": [
            "node_modules/*"
        ]
    }
},
"include": [
  "./**/*"
]
}
